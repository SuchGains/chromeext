{
  "manifest_version": 2,

  "name": "Stack Search",
  "description": "Search your questions on StackOverflow",
  "version": "1.0",

  "browser_action": {
    "default_icon": "stackoverflow-icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/",
    "https://api.stackexchange.com/docs/search"
  ]
}